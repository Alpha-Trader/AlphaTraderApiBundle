{
    "name": "alphatrader/api-bundle",
    "description": "API Bridge for AlphaTrader",
    "keywords": ["AlphaTrader", "stocks", "api", "bridge"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tr0nYx",
            "email": "tronyx@bric.finance"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "^6.1.1",
        "jms/serializer-bundle": "^1.1.0",
        "symfony/http-foundation": "^3.1",
        "doctrine/collections": "^1.3"
    },
    "require-dev": {
        "symfony/dependency-injection": "^3.1",
        "symfony/dom-crawler": "^3.1",
        "symfony/css-selector": "^3.1",
        "phpunit/phpunit": "~5.5",
        "squizlabs/php_codesniffer": "~2.5",
        "phpmd/phpmd": "~2.3",
        "sebastian/phpcpd": "~2.0",
        "satooshi/php-coveralls": "dev-master",
        "phpbench/phpbench": "~0.12.0"
    },
    "autoload": {
        "psr-4": {
            "Alphatrader\\ApiBundle\\": "src/",
            "Tests\\": "tests/AlphaTraderApiTests/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
